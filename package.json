{
  "name": "h5final",
  "version": "1.0.0",
  "description": "h5final project",
  "main": "index.js",
  "scripts": {
    "start": "node       lib/index.js",
    "start:ts": "ts-node lib-ts/index.ts",
    "build": "      rm -rf lib/* && tsc",
    "build:watch": "rm -rf lib/* && tsc --watch",
    "test": "         npm run build && jest --forceExit",
    "test:coverage": "npm run build && jest --forceExit --coverage",
    "test:watch": "   npm run build && jest --forceExit --watch",
    "tslint": "    tslint -c tslint.json       src/**/*.ts?(x)",
    "tslint:fix": "tslint -c tslint.json --fix src/**/*.ts?(x)",
    "dev": " rm -rf dev/* && webpack --progress --colors --config webpack/webpack.dll.js &&  webpack-dev-server --config webpack/webpack.dev.js --devtool eval --progress --colors --content-base dev",
    "prod": "gulp prod",
    "webpack": "  webpack  --progress --colors --config webpack/webpack.dev.js",
    "webpack:prod": " rm -rf prod/* && webpack --progress --colors --config webpack/webpack.dll.prod.js && webpack    --progress --colors       --config webpack/webpack.prod.js && gulp build:prod",
    "compress": "gulp build:prod",
    "doc": " rm -rf lib/* && tsc && gulp doc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-dove": "webpack --progress --colors --config dll/dove/webpack.js"
  },
  "author": "heartblood",
  "license": "ISC",
  "dependencies": {
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.4",
    "@types/lodash": "^4.14.66",
    "@types/node": "^8.0.2",
    "@types/react": "^15.0.31",
    "@types/react-dom": "^15.5.1",
    "antd": "^2.12.8",
    "awesome-typescript-loader": "^3.1.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-react-css-modules": "^3.0.0",
    "connect-gzip": "^0.1.6",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "ftps": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-uglify": "^3.0.0",
    "howler": "^2.0.4",
    "html-webpack-plugin": "^2.29.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "mobx-react-devtools": "^4.2.15",
    "node-fetch": "^1.7.1",
    "pump": "^1.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "style-loader": "^0.18.2",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "unfetch": "^3.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
